{
  "extends": [
    "solhint:recommended"
  ],
  "plugins": [
    "prettier"
  ],
  "rules": {
    "compiler-version": [
      "error",
      "^0.8.26"
    ],
    "func-visibility": [
      "error",
      {
        "ignoreConstructors": true
      }
    ],
    "private-vars-leading-underscore": "error",
    "max-line-length": [
      "warn",
      140
    ],
    "no-empty-blocks": "off",
    "not-rely-on-time": "off",
    "no-global-import": "off",
    "event-name-camelcase": "off",
    "prettier/prettier": "warn",
    "func-name-mixedcase": "off"
  }
}